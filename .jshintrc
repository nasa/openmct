{
    "bitwise": true,
    "browser": true,
    "curly": true,
    "eqeqeq": true,
    "forin": true,
    "freeze": true,
    "funcscope": false,
    "futurehostile": true,
    "latedef": true,
    "noarg": true,
    "nocomma": true,
    "nonbsp": true,
    "nonew": true,
    "predef": [
        "define",
        "Promise",
        "WeakMap",
        "Map"
    ],
    "shadow": "outer",
    "strict": "implied",
    "undef": true,
    "unused": "vars"
}
